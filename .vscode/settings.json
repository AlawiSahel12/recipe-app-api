{
  "python.analysis.autoImportCompletions": true,
  "pylint.enabled": false,
  "python.analysis.diagnosticSeverityOverrides": {
    "reportMissingImports": "none"
  },
  "files.exclude": {
    "**/__pycache__": true,
    "**/*.pyc": true,
    "**/*.pyo": true
  },
  "flake8.args": [
    "--exclude= migrations,__pycache__,manage.py,settings.py",
    "--max-line-length=90"
  ]
}
